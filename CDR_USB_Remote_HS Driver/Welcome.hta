<script language="javascript">

	// Set the fixed window size.
	var startWindowWidth  = 720;
	var startWindowHeight = 720;
	var startWindowLeft   = 200;
	var startWindowTop    = 100;

	// Check if the screen size info is available.
	if (window.screen)
	{
		// Calculate the real default window location.
		startWindowLeft = (window.screen.availWidth - startWindowWidth) / 2;
		startWindowTop  = (window.screen.availHeight - startWindowHeight) / 2;
	}

	// Center the start window on the screen.
	window.moveTo(startWindowLeft, startWindowTop);
	window.resizeTo(startWindowWidth, startWindowHeight);

    </script>

   <SCRIPT LANGUAGE="JavaScript">
		function runmsi(obj, strPathMsi)
		{			
			var objFSO = new ActiveXObject("Scripting.FileSystemObject");
			var strPath = unescape(document.location.pathname);
  			var file = objFSO.GetFile(strPath);
 			var strLoc = file.ParentFolder;
 			
			var objShell = new ActiveXObject("WScript.Shell");
			var strCmd = "msiexec -i \"" + strLoc + "\\" + strPathMsi + "\"";

			// launch it
			var lngReturn = objShell.Run (strCmd, 1, 1);
			objShell = null;
		}
				
		function runexe(obj, strPathExe)
		{			
			var objFSO = new ActiveXObject("Scripting.FileSystemObject");
			var strPath = unescape(document.location.pathname);
  			var file = objFSO.GetFile(strPath);
 			var strLoc = file.ParentFolder;
 			
			var objShell = new ActiveXObject("WScript.Shell");
			var strCmd = "\"" + strLoc + "\\" + strPathExe + "\"";

			// launch it
			var lngReturn = objShell.Run (strCmd, 1, 1);
			objShell = null;
		}
				
	 </SCRIPT>


     <SCRIPT FOR="cmdInstall" EVENT="onclick" LANGUAGE="javascript">
		runexe(this, "CDR USB Remote HS Driver Setup.exe");
